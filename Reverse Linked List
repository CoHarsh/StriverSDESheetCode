LinkedListNode<int> *reverseLinkedList(LinkedListNode<int> *head) 
{
        LinkedListNode<int>* current = head;
        LinkedListNode<int> *prev = NULL, *next = NULL;
 
        while (current != NULL) {
            next = current->next;
            current->next = prev;
            prev = current;
            current = next;
        }
        return prev;
}
